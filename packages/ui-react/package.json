{
  "name": "@block-editor/react",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles.css": "./dist/editor.css"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && pnpm build:css",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.eslintcache --ignore-pattern .gitignore --fix",
    "lint-check": "eslint . --ext .ts,.tsx --cache --cache-location node_modules/.eslintcache --ignore-pattern .gitignore",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-unit": "vitest run --project unit",
    "test-integ": "vitest run --project integration",
    "build:css": "npx tailwindcss -i src/index.css -o dist/editor.css --minify"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "@block-editor/core": "workspace:*",
    "@floating-ui/react": "^0.27.16",
    "motion": "^12.26.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "jsdom": "^27.4.0",
    "tailwindcss": "^4.1.18",
    "tsc-alias": "^1.8.16",
    "vitest": "^4.0.16"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1"
}

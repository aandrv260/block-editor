{
  "name": "@block-editor/core",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts --cache --cache-location node_modules/.eslintcache --ignore-pattern .gitignore --fix",
    "lint-check": "eslint . --ext .ts --cache --cache-location node_modules/.eslintcache --ignore-pattern .gitignore",
    "test": "vitest run",
    "test-watch": "vitest",
    "test-unit": "vitest run --project unit",
    "test-integ": "vitest run --project integration",
    "test-coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1",
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.16",
    "tsc-alias": "^1.8.10",
    "vitest": "^4.0.16"
  }
}
